{
  "name": "simple-oauth-redirect",
  "types": "./types.d.ts",
  "version": "1.1.0",
  "description": "Provides simple functions for initiating and handling local OAuth redirects for both Node and Browser",
  "scripts": {
    "build": "node ./build.mjs",
    "prepublish": "npm run build"
  },
  "keywords": [
    "oauth",
    "redirect",
    "openid"
  ],
  "author": "md-y",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/md-y/simple-redirect/issues"
  },
  "homepage": "https://github.com/md-y/simple-redirect",
  "optionalDependencies": {
    "open": "^8.4.2"
  },
  "devDependencies": {
    "@types/node": "^18.15.2",
    "tsup": "^8.0.1",
    "typescript": "^5.0.4"
  },
  "exports": {
    "./web": {
      "import": "./web.mjs",
      "require": "./web.cjs",
      "types": "./types.d.ts"
    },
    "./node": {
      "import": "./node.mjs",
      "require": "./node.cjs",
      "types": "./types.d.ts"
    }
  }
}
