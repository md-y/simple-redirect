{
  "name": "simple-oauth-redirect",
  "types": "./types.d.ts",
  "version": "0.0.2",
  "description": "Provides simple functions for initiating and handling local OAuth redirects for both Node and Browser",
  "scripts": {
    "build": "tsc --outDir ./dist/esm && tsc --outDir ./dist/cjs --module CommonJS",
    "prepublish": "npm run build"
  },
  "keywords": [
    "oauth",
    "redirect",
    "openid"
  ],
  "author": "md-y",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/md-y/simple-redirect/issues"
  },
  "homepage": "https://github.com/md-y/simple-redirect",
  "optionalDependencies": {
    "open": "^8.4.2"
  },
  "devDependencies": {
    "@types/node": "^18.15.2",
    "typescript": "^5.0.4"
  },
  "exports": {
    "./web": {
      "default": {
        "default": "./dist/esm/web.js",
        "import": "./dist/esm/web.js",
        "require": "./dist/cjs/web.js",
        "types": "./types.d.ts"
      }
    },
    "./node": {
      "default": {
        "default": "./dist/cjs/node.js",
        "import": "./dist/esm/node.js",
        "require": "./dist/cjs/node.js",
        "types": "./types.d.ts"
      }
    }
  }
}
